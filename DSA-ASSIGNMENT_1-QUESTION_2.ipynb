   "source": [
    "li=[1,2,3,4,5,4]\n",
    "\n",
    "def reverse_list(li):\n",
    "  full_len=len(li)\n",
    "  half_len=(int)(full_len/2)\n",
    "\n",
    "  for i in range(half_len):\n",
    "    temp=li[i]\n",
    "    li[i]=li[full_len-1-i]\n",
    "    li[full_len-1-i]=temp\n",
    "\n",
    "  print(li)\n",
    "\n",
    "reverse_list(li)"
   ]
